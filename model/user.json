{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://www.mocky.io/v2/5808862710000087232b75ac",
  "type": "object",
  "title": "The Root Schema",
  "required": ["clients"],
  "properties": {
    "clients": {
      "$id": "#/properties/clients",
      "type": "array",
      "title": "The Clients Schema",
      "items": {
        "$id": "#/properties/clients/items",
        "type": "object",
        "title": "The Items Schema",
        "required": ["id", "name", "email", "role"],
        "properties": {
          "id": {
            "$id": "#/properties/clients/items/properties/id",
            "type": "string",
            "title": "The Id Schema",
            "default": "",
            "examples": ["a0ece5db-cd14-4f21-812f-966633e7be86"],
            "pattern": "^(.*)$"
          },
          "name": {
            "$id": "#/properties/clients/items/properties/name",
            "type": "string",
            "title": "The Name Schema",
            "default": "",
            "examples": ["Britney"],
            "pattern": "^(.*)$"
          },
          "email": {
            "$id": "#/properties/clients/items/properties/email",
            "type": "string",
            "title": "The Email Schema",
            "default": "",
            "examples": ["britneyblankenship@quotezart.com"],
            "pattern": "^(.*)$"
          },
          "role": {
            "$id": "#/properties/clients/items/properties/role",
            "type": "string",
            "title": "The Role Schema",
            "default": "",
            "examples": ["admin"],
            "pattern": "^(.*)$"
          }
        }
      }
    }
  }
}
